<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Stylist</title>
  </head>
  <body>
    <h1>All Stylist</h1>
    @if (@Model.Count == 0)
    {
      <p>There are no Stylist yet.</p>
    }
    <ul>
      @foreach (var stylist in Model)
      {
        <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</a></li>
      }
    </ul>
    <a href="/stylist/new">Add New Stylist</a>

  </body>
</html>
